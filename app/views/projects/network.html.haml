- if private_check
  - content_for :title do
    = @project.name.titleize

  %article.project{ data: 'network' }
    = render 'project_header'
    = render 'project_toolbar'
    = render 'shared/messages'

    %section
      %div
        = nested_projects([@root], @project)
- else
  = render 'permissions_error'
