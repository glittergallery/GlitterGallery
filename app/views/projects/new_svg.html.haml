- unless private_check
  = render 'private_error'
- else
 
  - content_for :title do
    New SVG

  - content_for :pagetitle do
    = link_to @project.name.titleize, @project.urlbase
    \/
    = 'New SVG'

  = render 'project_toolbar'
  %br
    
  = form_tag create_svg_project_path do
    What are you calling your sketch?
    = text_field_tag :filename
    = sketchily_tag :sketch
    = submit_tag
