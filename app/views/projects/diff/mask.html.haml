- content_for :title do
  Mask

%article.project{ data: 'current' }
  = render 'project_header'
  = render 'project_toolbar'
  = render 'shared/messages'
  = render 'breadcrumb'

  %section
    - diff1 = @bloblist.first
    - diff2 = @bloblist.second
    comparing
    = diff1.second.oid[0..7] + '..' + diff2.second.oid[0..7]
    #mask
      #mask_first
        = data_image_tag(diff1.first, '100%', nil)
      #mask_second
        = data_image_tag(diff2.first, nil, nil, 'mask_top')

    #mask_slider
